namespace org.z2legacy.samples.ut.eq01;

class TestAutoEqAutoNq {
	def @main() {
		val a = Foo{4};
		val b = Foo{4};
		val c = Foo{6};

		System.Out << (a == b) << "\n";
		System.Out << (a == c) << "\n";
		System.Out << (a != b) << "\n";
		System.Out << (a != c) << "\n";
	}
}

class Foo {
	this(t: Int) {
		a = t;
		b = t;
	}
}
private {
	val a: Int;
	val b: Int;
}