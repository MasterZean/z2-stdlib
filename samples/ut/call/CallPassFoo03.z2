namespace org.z2legacy.ut.call;

using Foo;

class CallPassFoo03 {
	def test(const a: Foo) {
		System.Out << "const\n";
		g(a);
	}
	
	def test(ref a: Foo) {
		System.Out << "reference\n";
		g(a);
	}
	
	def g(const a: Foo) {
		System.Out << "g const\n";
	}
	
	def g(ref a: Foo) {
		System.Out << "g reference\n";
	}
	
	def g(move a: Foo) {
		System.Out << "g move\n";
	}
	
	def @main() {
		val a = Foo{};
		test(Foo{});
		test(a);
	}
}