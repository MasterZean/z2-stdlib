namespace org.z2legacy.samples.container.carray;

class PassFill01 {
	def print(const a: [c]Int) {
		for (val i = 0p; i < a.Length; i++)
			System.Out << '[' << i << "]=" << a[i] << " ";
		System.Out << '\n';
	}

	def @main() {
		val a = [c 7]Int{};
		print(a);
		val b = [c 7]{ 7, ... };
		print(b);
		b.Fill(9);
		print(b);
		b = a;
		print(b);
		a.Fill(-1);
		val c: [c 3]Int = a;
		print(c);
		val d: [c 14]Int = a;
		print(d);
	}
}