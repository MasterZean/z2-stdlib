namespace org.z2legacy.samples.container.carray;

class PassInit01 {
	def @main() {
		val a = CArray<Int, 4>{};
		System.Out << a << "\n";
		
		val b: CArray<Int, 4>;
		System.Out << b << "\n";
		
		val c: [c 4]Int;
		System.Out << c << "\n";
		
		val d = [c 4]{ 0, 0, 0, 0 };
		System.Out << d << "\n";
		
		val e = [c]{ 0, 0, 0, 0 };
		System.Out << e << "\n";
		
		System.Out << a.class << "\n";
		System.Out << a[0].class << "\n";
	}
}