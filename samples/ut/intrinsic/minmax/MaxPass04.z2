namespace org.z2legacy.samples.ut.intrinsic.minmax;

class MaxPass04 {
	def @main() {
		val va = Intrinsic.Max(5, 20);
		System.Out << va.class << " max is " << va << "\n";
		
		val vb = Intrinsic.Max(50u, 200u);
		System.Out << vb.class << " max is " << vb << "\n";
		
		val vc = Intrinsic.Max(500l, 2000l);
		System.Out << vc.class << " max is " << vc << "\n";
		
		val vd = Intrinsic.Max(5000ul, 20000ul);
		System.Out << vd.class << " max is " << vd << "\n";
		
		val vi = Intrinsic.Max(0.4f, 22.1f);
		System.Out << vi.class << " max is " << vi << "\n";
		
		val vj = Intrinsic.Max(0.04000000, 22.01);
		System.Out << vj.class << " max is " << vj << "\n";
		
		val vk = Intrinsic.Max(-0.04000000, -22.01);
		System.Out << vk.class << " max is " << vk << "\n";
		
		val vl = Intrinsic.Max('a', '7');
		System.Out << vl.class << " max is " << vl << "\n";
	}
}