namespace sys.core;

using Stream;

class Class {
	@[intrinsic]
	property Name: ref const String;
	
	@[intrinsic]
	property ShortName: ref const String;
	
	@[intrinsic]
	property Namespace: ref const String;

#region IO

	func @write(ref stream: Stream) {
		stream.RawWrite(Name);
	}

#endregion
}